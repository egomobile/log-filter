{
    "name": "@egomobile/log-filter",
    "version": "0.1.1",
    "description": "Extension for js-log with utilities to handle extended filters.",
    "main": "lib/index.js",
    "engines": {
        "node": ">=12.0.0"
    },
    "scripts": {
        "build": "del ./lib && tsc",
        "docs:generate": "del ./docs && typedoc --out ./docs ./src",
        "lint": "eslint -c .eslintrc.js --ext .ts src",
        "lint:fix": "eslint --fix -c .eslintrc.js --ext .ts src",
        "sandbox": "nodemon --watch './sandbox/index.ts' ./sandbox/index.ts",
        "test": "jest --no-cache --silent"
    },
    "jest": {
        "preset": "ts-jest",
        "testEnvironment": "node",
        "testMatch": [
            "**/__tests__/**/*.test.ts"
        ],
        "setupFilesAfterEnv": [
            "./src/setupTests.ts"
        ]
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/egomobile/log-filter"
    },
    "author": "Next.e.GO Mobile SE, Aachen, Germany",
    "license": "LGPL-3.0",
    "files": [
        "CHANGELOG.md",
        "LICENSE",
        "lib",
        "!lib/**/__tests__/**/*",
        "!lib/**/*.map",
        "!lib/**/setupTests.*",
        "README.md"
    ],
    "dependencies": {
        "@types/node": "12.20.27",
        "dayjs": "1.11.0",
        "filtrex": "2.2.3"
    },
    "devDependencies": {
        "@egomobile/log": "0.3.0",
        "@egomobile/tsconfig": "5.0.0",
        "@types/jest": "27.0.3",
        "@types/supertest": "2.0.11",
        "babel-jest": "27.4.4",
        "del-cli": "4.0.1",
        "dotenv": "16.0.0",
        "eslint": "7.32.0",
        "eslint-config-ego": "0.15.0",
        "jest": "27.4.4",
        "nodemon": "2.0.15",
        "supertest": "6.1.6",
        "ts-jest": "27.1.1",
        "ts-node": "10.7.0",
        "tslint": "6.1.3",
        "typedoc": "0.22.15",
        "typescript": "4.4.4"
    },
    "peerDependencies": {
        "@egomobile/log": "0.3.0"
    }
}
